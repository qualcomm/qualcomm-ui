  "scripts": {
    "build:prod": "./scripts/build-prod.sh",
    "build": "run-s build:typedoc build:web",
    "build:web": "run-s build:angular copy-build build:site",
    "build:angular": "ng build",
    "build:site": "react-router build",
    "build:typedoc": "qui-typedoc build",
    "watch:typedoc": "qui-typedoc watch",
    "dev": "run-s check-angular-demo-module compile:watch",
    "check-angular-demo-module": "shx test -f dist/main.js || pnpm build:angular",
    "compile:watch": "run-p dev:angular dev:demo-chunks dev:app typecheck",
    "dev:angular": "ng build --watch --configuration development",
    "dev:demo-chunks": "tsx scripts/watch-angular-demo-chunks.ts",
    "dev:app": "vite -c vite.config.ts",
    "watch:angular": "run-p dev:angular dev:demo-chunks",
    "watch:app": "run-p dev:app typecheck",
    "copy-build": "tsx scripts/copy-angular-demo-chunks.ts",
    "typecheck": "react-router typegen && tsc -w --preserveWatchOutput -p tsconfig.lib.json",
    "lint": "eslint --quiet",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "generate-page-map": "qui-mdx-vite generate-page-map -o ./scripts/temp/site-data.json",
    "build-demo-html": "tsx scripts/build-demo-html.ts",
    "update-knowledge-saga": "run-s generate-knowledge-saga upload-knowledge-saga",
    "update-knowledge-qui": "run-s generate-knowledge-qui upload-knowledge-qui",
    "generate-knowledge": "qui-mdx-vite generate-llms-txt --clean --output-mode per-page --exclude 'setup*' 'theme*' 'legacy*' 'changelogs*' 'index*' 'troubleshooting*' 'integrations*' 'installation*' 'introduction*'",
    "upload-knowledge": "run-p upload-knowledge-saga upload-knowledge-qui",
    "generate-knowledge-saga": "pnpm generate-knowledge --env .env.saga-ai",
    "upload-knowledge-saga": "qui-mdx-vite upload-knowledge --env .env.saga-ai",
    "generate-knowledge-qui": "pnpm generate-knowledge --env .env.qui-ai",
    "upload-knowledge-qui": "qui-mdx-vite upload-knowledge --env .env.qui-ai"
  },
