  "license": "BSD-3-Clause-Clear",
  "repository": "https://github.com/qualcomm/qualcomm-ui",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "run-s build:typedoc build:site",
    "build:site": "react-router build --mode production",
    "build:typedoc": "qui-typedoc build",
    "watch:typedoc": "qui-typedoc watch",
    "dev": "run-p dev:app typecheck",
    "dev:app": "vite -c vite.config.ts",
    "lint": "eslint --quiet",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc -w --preserveWatchOutput",
    "update-knowledge": "run-s generate-knowledge-saga generate-knowledge-qui upload-knowledge",
    "generate-knowledge": "qui-mdx-vite generate-llms-txt --clean --output-mode per-page --base-url 'https://react-next.qui.qualcomm.com' --exclude 'setup*' 'theme*' 'legacy*' 'changelogs*' 'troubleshooting*' 'integrations*' 'installation*' 'index*'",
    "upload-knowledge": "run-p upload-knowledge-saga upload-knowledge-qui",
    "generate-knowledge-saga": "pnpm generate-knowledge --env .env.saga-ai",
    "upload-knowledge-saga": "qui-mdx-vite upload-knowledge --env .env.saga-ai",
    "generate-knowledge-qui": "pnpm generate-knowledge --env .env.qui-ai",
    "upload-knowledge-qui": "qui-mdx-vite upload-knowledge --env .env.qui-ai",
    "update-lazy-demo-map": "qui-mdx-vite generate-lazy-demo-map -r src/routes -o src/components/demo/lazy-demo-loader.ts"
  },
