{
  "name": "@qualcomm-ui/eslint-plugin-path-alias",
  "version": "1.0.1",
  "description": "ESLint config for restricting import boundaries to tsconfig paths",
  "author": "Ryan Bower",
  "license": "BSD-3-Clause-Clear",
  "repository": "https://github.com/qualcomm/qualcomm-ui",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "config"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "plugin.js",
    "rules/"
  ],
  "scripts": {
    "test": "vitest -c vitest.config.ts --run",
    "test:unit:ci": "cross-env NODE_OPTIONS='' vitest -c vitest.config.ts --run --pool=threads --no-file-parallelism",
    "prepublishOnly": "qui-cli pre-publish",
    "postpublish": "qui-cli post-publish"
  },
  "devDependencies": {
    "@qualcomm-ui/cli": "workspace:^1.0.3",
    "@qualcomm-ui/tsconfig": "workspace:*",
    "@typescript-eslint/utils": "catalog:",
    "@typescript-eslint/rule-tester": "^8.31.1",
    "typescript-eslint": "catalog:",
    "vitest": "^3.2.4"
  }
}
