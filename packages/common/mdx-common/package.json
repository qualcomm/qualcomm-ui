{
  "name": "@qualcomm-ui/mdx-common",
  "version": "1.2.0",
  "description": "MDX common types and utilities",
  "repository": "https://github.com/qualcomm/qualcomm-ui",
  "author": "Ryan Bower",
  "license": "BSD-3-Clause-Clear",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean build:js build:ts",
    "clean": "shx rm -rf dist/tsbuildinfo dist/**/*.js dist/**/*.cjs dist/**/*.mjs dist/**/*.js.map",
    "build:js": "tsx build.ts",
    "build:ts": "tsc -b tsconfig.lib.json",
    "dev": "run-p watch:js watch:ts",
    "watch:js": "tsx build.ts --watch",
    "watch:ts": "pnpm build:ts -w --preserveWatchOutput",
    "lint": "eslint --quiet",
    "prepublishOnly": "qui-cli pre-publish",
    "postpublish": "qui-cli post-publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@qualcomm-ui/cli": "workspace:^1.0.5",
    "@qualcomm-ui/esbuild": "workspace:^1.0.3",
    "@qualcomm-ui/tsconfig": "workspace:^1.0.4",
    "@qualcomm-ui/typedoc-common": "workspace:^1.0.3",
    "esbuild": "^0.25.8",
    "fuzzysort": "^3.0.1",
    "shx": "^0.4.0",
    "tsx": "^4.20.6",
    "vite": "npm:rolldown-vite@7.2.6"
  },
  "peerDependencies": {
    "@qualcomm-ui/typedoc-common": "workspace:^1.0.3",
    "fuzzysort": ">=3"
  }
}
