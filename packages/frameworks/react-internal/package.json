{
  "name": "@qualcomm-ui/react-internal",
  "version": "0.0.0",
  "description": "internal styles and utilities for documentation, used by both React and Angular documentation modules",
  "author": "Ryan Bower",
  "license": "BSD-3-Clause-Clear",
  "repository": "https://github.com/qualcomm/qualcomm-ui",
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js",
      "default": "./dist/*/index.js"
    }
  },
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:js build:ts",
    "clean": "shx rm -rf dist/tsbuildinfo dist/**/*.js dist/**/*.cjs dist/**/*.mjs dist/**/*.js.map",
    "build:js": "tsx build.ts",
    "build:ts": "tsc -b tsconfig.lib.json",
    "dev": "run-p watch:js watch:ts",
    "watch:js": "tsx build.ts --watch --mode development",
    "watch:ts": "pnpm build:ts -w --preserveWatchOutput",
    "lint": "eslint --quiet",
    "prepublishOnly": "qui-cli pre-publish",
    "postpublish": "qui-cli post-publish"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "esbuild": "^0.25.8",
    "@floating-ui/react": "^0.27.3",
    "@qualcomm-ui/cli": "workspace:*",
    "@qualcomm-ui/core": "workspace:*",
    "@qualcomm-ui/dom": "workspace:*",
    "@qualcomm-ui/esbuild": "workspace:*",
    "@qualcomm-ui/react-core": "workspace:*",
    "@qualcomm-ui/react-router-utils": "workspace:*",
    "@qualcomm-ui/react-test-utils": "workspace:*",
    "@qualcomm-ui/tsconfig": "workspace:*",
    "@qualcomm-ui/utils": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "babel-plugin-react-compiler": "^1.0.0",
    "lucide-react": "0.525.0",
    "quick-lru": "^7.2.0",
    "react": "catalog:",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "catalog:",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "npm:rolldown-vite@7.2.6"
  }
}
