{
  "name": "@qualcomm-ui/angular",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/frameworks/angular/src",
  "prefix": "qui",
  "projectType": "library",
  "targets": {
    "build": {
      "cache": true,
      "dependsOn": [
        {
          "projects": ["@qualcomm-ui/angular-core"],
          "target": "build"
        }
      ],
      "executor": "@nx/angular:ng-packagr-lite",
      "options": {
        "project": "{projectRoot}/src/ng-package.json"
      }
    },
    "build:uncached": {
      "dependsOn": [
        {
          "projects": ["@qualcomm-ui/angular-core"],
          "target": "build:uncached"
        }
      ],
      "executor": "@nx/angular:ng-packagr-lite",
      "options": {
        "project": "{projectRoot}/src/ng-package.json"
      }
    },
    "dev": {
      "dependsOn": [
        {
          "projects": ["@qualcomm-ui/angular-core"],
          "target": "build:uncached"
        }
      ],
      "executor": "@nx/angular:ng-packagr-lite",
      "options": {
        "project": "{projectRoot}/src/ng-package.json",
        "watch": true
      }
    }
  }
}
